<div class="special_dates">
  <h1>Special Dates</h1>

  <ul>
    <% hours.each do |key, value| %>
      <li>
        <b><%= "#{value["formatted_dates"]}" %></b>
        <ul>
          <li><%= "#{value["desc"]}: #{value["formatted_hours"]}" %></li>
        </ul>
      </li>
    <% end %>
  </ul>
</div>


